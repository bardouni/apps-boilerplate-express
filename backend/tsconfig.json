{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "outDir": "./bundle",
    "rootDir": "./",
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "preserve",
    "lib":[
      "es2019",
      "dom"
    ]
  },
  "exclude":[]
}
